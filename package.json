{
  "name": "a-notion-loader",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "bun@1.2.18",
  "description": "Astro content loader for notion!",
  "author": "Limx <i@limx.fun>",
  "license": "MIT",
  "homepage": "https://github.com/Liumingxun/a-notion-loader",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Liumingxun/a-notion-loader.git"
  },
  "keywords": [
    "withastro",
    "astro-loader",
    "content-layer",
    "notion"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "workspaces": {
    "packages": [
      "example"
    ],
    "catalog": {
      "@antfu/eslint-config": "^4.16.2",
      "@notionhq/client": "~4.0.2",
      "@types/bun": "1.2.20",
      "astro": "^5.11.0",
      "eslint": "^9.24.0",
      "eslint-plugin-astro": "^1.3.1",
      "eslint-plugin-format": "^1.0.1",
      "prettier-plugin-astro": "^0.14.1",
      "ts-to-zod": "^3.15.0",
      "typescript": "5.8.3",
      "astro-eslint-parser": "^1.2.2",
      "bunup": "^0.8.60"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bun -F example -b dev",
    "lint": "eslint --fix",
    "prebuild": "bun run ./scripts/zodify.ts",
    "build": "bunup -f esm,cjs src/index.ts"
  },
  "peerDependencies": {
    "astro": "catalog:"
  },
  "dependencies": {
    "@notionhq/client": "catalog:",
    "ts-morph": "^26.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@types/bun": "catalog:",
    "astro-eslint-parser": "catalog:",
    "bunup": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-astro": "catalog:",
    "eslint-plugin-format": "catalog:",
    "prettier-plugin-astro": "catalog:",
    "ts-to-zod": "catalog:",
    "typescript": "catalog:"
  }
}
